{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "routePrefix" : "api",
  "apiVer" : "v1",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mongodb": "mongodb://localhost:27017/plannerdb",
  "ssl": {
    "enabled": false,
    "sslCertPath": "../.sslcert/"
  },  
  "authentication": {
    "enabled": true,
    "secret": "",
    "strategies": [
      "jwt",
      "local",
      "auth0"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "http://localhost:3030/api/v1",
      "subject": "anonymous",
      "issuer": "https://xplanner.auth0.com/",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    },
    "auth0": {
      "clientID": "vcE5nGaE1cIgX7XbdHdgQdc4TMEkJLju",
      "clientSecret": "DzXQH4CCy0G73OTJVDEXL9HU-vAiLU7Pf8lIrzr1tD9K03iybDR_vteZFHNyvhzL",
      "successRedirect": "/success.html",
      "domain": "xplanner.auth0.com",
      "responseType": "code",
      "audience": "https://xplanner.auth0.com/userinfo",
      "scope": "openid profile"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  }
}
